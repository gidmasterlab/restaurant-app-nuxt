		"eslint-plugin-prettier": "^5.1.2",
		"http-server": "^14.1.1",
		"nuxt": "^3.8.2",
		"nuxt-svgo": "^3.7.0",
		"prettier": "^3.1.1",
		"sass": "^1.69.5",
		"typescript": "^5.3.3",
		"vue": "^3.3.12",
		"vue-router": "^4.2.5"
